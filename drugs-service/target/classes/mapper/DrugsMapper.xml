            <result property="drugsretailprice" column="drugsRetailPrice" jdbcType="VARCHAR"/>
            <result property="drugscreattime" column="drugsCreatTime" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        drugsId,drugsName,drugsTypeId,
        drugsSpecifications,drugsUnitId,drugsPlace,
        drugsOriginPrice,drugsRetailPrice,drugsCreatTime
